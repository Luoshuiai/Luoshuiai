    <item>
      <title>网页知识库的搭建</title>
      <link>https://fhlp.me/posts/pkm/%E7%9F%A5%E8%AF%86%E7%AE%A1%E7%90%86/%E7%BD%91%E9%A1%B5%E7%9F%A5%E8%AF%86%E5%BA%93%E7%9A%84%E6%90%AD%E5%BB%BA/</link>
